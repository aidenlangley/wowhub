  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://wowhub.co.nz",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/aidenlangley/wowhub.co.nz"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "dev:host": "svelte-kit dev --host",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "build:preview": "pnpm run build && pnpm run preview",
    "deploy": "git add docs && git commit -m 'ðŸš€ deploy docs!' && git push",
    "lint": "pnpm run check && pnpm run eslint && pnpm run stylelint",
    "lint:fix": "pnpm run eslint:fix && pnpm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint '**/*.{css,svelte}'",
    "stylelint:fix": "stylelint --fix '**/*.{css,svelte}'",
    "prettier": "prettier --check --plugin-search-dir=. .",
    "prettier:format": "prettier --write . --plugin-search-dir=. .",
    "lint:build": "pnpm run lint && pnpm run build",
    "lint:build:deploy": "pnpm run lint && pnpm run build && pnpm run deploy"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.19",
    "@sveltejs/kit": "^1.0.0-next.169",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.3.4",
    "cssnano": "^5.0.6",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "mdsvex": "^0.9.8",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.1.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rehype-external-links": "^1.0.0",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^2.0.0",
    "remark-images": "^3.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "svelte": "^3.42.6",
    "svelte-check": "^2.2.6",
    "svelte-image": "^0.2.9",
    "svelte-preprocess": "^4.9.4",
    "svelte-seo": "^1.3.0",
    "tailwindcss": "^2.2.15",
    "tslib": "^2.0.0",
    "typescript": "~4.3.5"
  },
  "type": "module"
