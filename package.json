{
	"name": "wowhub",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev --host",
		"svelte:build": "svelte-kit build",
		"svelte:build:deploy": "svelte-kit build && npm run deploy",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"prettier:format": "prettier --write . --plugin-search-dir=. .",
		"lint": "npm run check && prettier --check --plugin-search-dir=. . && eslint . && stylelint .",
		"lint:es": "eslint .",
		"build": "npm run prettier:format && npm run lint && svelte-kit build",
		"build:deploy": "npm run build && npm run deploy",
		"build:preview": "npm run build && npm run preview",
		"deploy": "git add docs && git commit -m 'ðŸš€ deploy docs!' && git push",
		"preview": "svelte-kit preview"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.17",
		"@sveltejs/kit": "^1.0.0-next.158",
		"@tailwindcss/forms": "^0.3.3",
		"@tailwindcss/typography": "^0.4.1",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"autoprefixer": "^10.3.3",
		"cssnano": "^5.0.6",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"heroicons": "^1.0.4",
		"mdsvex": "^0.9.8",
		"postcss": "^8.3.5",
		"postcss-import": "^14.0.2",
		"postcss-load-config": "^3.1.0",
		"postcss-nesting": "^8.0.1",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rehype-dom-stringify": "^3.0.0",
		"rehype-external-links": "^1.0.0",
		"remark-footnotes": "^4.0.1",
		"remark-gfm": "^2.0.0",
		"sass": "^1.38.2",
		"svelte": "^3.42.4",
		"svelte-check": "^2.2.5",
		"svelte-hero-icons": "^2.4.0",
		"svelte-preprocess": "^4.7.4",
		"tailwindcss": "^2.2.8",
		"tslib": "^2.0.0",
		"typescript": "~4.3.5"
	},
	"type": "module",
	"dependencies": {}
}
